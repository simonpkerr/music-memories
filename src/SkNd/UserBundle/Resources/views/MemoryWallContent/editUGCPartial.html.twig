<form id="edit-ugc-form" class="full-width" action="{{ path('memoryWallEditUGC',{'id': id, 'mwid':mwid, 'slug':slug }) }}" {{ form_enctype(form) }} method="POST">
    <fieldset>
    <legend>Update some stuff</legend>
    {{ form_widget(form) }}
    <div class="controls">
        <input type="reset" class="grey button" value="{{ 'memoryWall.ugc.form.reset'|trans({}, 'SkNdUserBundle') }}" />
        <input type="submit" value="{{ 'memoryWall.ugc.edit.submit'|trans({}, 'SkNdUserBundle') }}" />
    </div>
    </fieldset>
</form>
<div id="ugc-progress">
    <div id="bar"></div>
</div>